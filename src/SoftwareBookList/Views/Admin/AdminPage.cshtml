@using SoftwareBookList.Models
@model List<Book>

<h1>Admin Page</h1>

@if (User.IsInRole("Admin"))
{
    <form method="post" asp-action="PopulateBookData">
        <button type="submit" class="btn btn-primary">Populate Book Data</button>
    </form>
}
else
{
    <p>You do not have permission to access this page.</p>
}
